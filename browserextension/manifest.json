  "background": {
      "page": "background.html",
      "persistent": true
   },

   "content_security_policy": "[script-src 'self'; object-src 'self']",

  "manifest_version": 2,

  "name": "Bubble",
  "version": "0.1",
  "description": "Bubble Browser Extension",

  "permissions": [
   "notifications",
   "http://0.0.0.0:5000/"
  ],

  "browser_action": {
    "default_popup" : "index.html"
  }

}
