{
  "background": {
     "page": "background.html",
      "persistent": true
  },

  "manifest_version": 2,

  "name": "Bubble",
  "version": "0.1",
  "description": "Bubble Browser Extension",

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "browser_action": {
    "default_title": "Bubble",
    "default_icon": "icon.png"
  }
}
